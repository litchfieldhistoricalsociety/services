<?xml version="1.0" encoding="UTF-8"?>
<tenant:TenantBindingConfig
        xmlns:merge='http://xmlmerge.el4j.elca.ch'
        xmlns:tenant='http://collectionspace.org/services/config/tenant'>

    <tenant:tenantBinding id="8" name="litchfieldhistoricalsociety.org" displayName="Litchfield Historical Society Tenant" version="0.1">
        <tenant:repositoryDomain name="default-domain" storageName="lhs-domain" repositoryClient="nuxeo-java" />

        <tenant:serviceBindings merge:matcher="id" id="CollectionObjects">
            <service:DocHandlerParams xmlns:service="http://collectionspace.org/services/config/service" merge:matcher="tag" merge:action="replace">
                <service:params>
                    <service:SupportsHierarchy>true</service:SupportsHierarchy>
                    <service:RefnameDisplayNameField> <!-- The field used as the display name in an object's refname -->
                        <service:element>objectNumber</service:element>
                        <service:xpath>objectNumber</service:xpath>
                    </service:RefnameDisplayNameField>
                    <service:ListResultsFields>
                        <service:ListResultField>
                            <service:element>objectNumber</service:element>
                            <service:xpath>objectNumber</service:xpath>
                        </service:ListResultField>
                        <service:ListResultField>
                            <service:element>objectName</service:element>
                            <service:xpath>objectNameList/[0]/objectName</service:xpath>
                        </service:ListResultField>
                        <service:ListResultField>
                            <service:element>objectProductionPlace</service:element>
                            <service:xpath>objectProductionPlaceGroupList/[0]/objectProductionPlace</service:xpath>
                        </service:ListResultField>
                        <service:ListResultField>
                            <service:element>objectProductionPerson</service:element>
                            <service:xpath>objectProductionPersonGroupList/[0]/objectProductionPerson</service:xpath>
                        </service:ListResultField>
                        <service:ListResultField>
                            <service:element>catalogingDateMade</service:element>
                            <service:schema>collectionobjects_lhs</service:schema>
                            <service:xpath>catalogingDateMade</service:xpath>
                        </service:ListResultField>
                    </service:ListResultsFields>
                </service:params>
            </service:DocHandlerParams>
            <service:object xmlns:service="http://collectionspace.org/services/config/service" >
                <service:part id="1" control_group="Managed" versionable="true" auditable="false" label="collectionobjects_common" updated="" order="1" merge:matcher="id" merge:action="merge">
                    <service:properties>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types" merge:matcher="skip" merge:action="insert">
                            <types:key>authRef</types:key>
                            <types:value>materialGroupList/*/material</types:value>
                        </types:item>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types" merge:matcher="skip" merge:action="insert">
                            <types:key>termRef</types:key>
                            <types:value>objectInscriptionGroupList/*/objectInscriptionType</types:value>
                        </types:item>
                    </service:properties>
                </service:part>

                <service:part id="3" merge:matcher="skip" merge:action="insert" control_group="Managed" versionable="true" auditable="false" label="collectionobjects_lhs" updated="" order="3">
                    <service:properties>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                            <types:key>authRef</types:key>
                            <types:value>formerOwner</types:value>
                        </types:item>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                            <types:key>termRef</types:key>
                            <types:value>legalOwner</types:value>
                        </types:item>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                            <types:key>termRef</types:key>
                            <types:value>category</types:value>
                        </types:item>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                            <types:key>termRef</types:key>
                            <types:value>cultures|culture</types:value>
                        </types:item>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                            <types:key>termRef</types:key>
                            <types:value>legalStatus</types:value>
                        </types:item>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                            <types:key>termRef</types:key>
                            <types:value>catalogingDateCenturies|catalogingDateCentury</types:value>
                        </types:item>
                    </service:properties>
                    <service:content contentType="application/xml">
                        <service:xmlContent namespaceURI="http://collectionspace.org/services/collectionobject/local/lhs" schemaLocation="http://collectionspace.org/services/collectionobject/local/lhs http://collectionspace.org/services/collectionobject/local/collectionobjects_lhs.xsd" />
                    </service:content>
                </service:part>

            </service:object>
        </tenant:serviceBindings>

        <tenant:serviceBindings merge:matcher="id" id="Conditionchecks">
            <service:DocHandlerParams xmlns:service="http://collectionspace.org/services/config/service" merge:matcher="tag" merge:action="replace">
                <service:params>
                    <service:ListResultsFields>
                        <service:ListResultField>
                            <service:element>conditionCheckRefNumber</service:element>
                            <service:xpath>conditionCheckRefNumber</service:xpath>
                        </service:ListResultField>
                        <service:ListResultField merge:matcher="skip" merge:action="insert">
                            <service:element>conditionLHS</service:element>
                            <service:schema>conditionchecks_lhs</service:schema>
                            <service:xpath>conditionLHS</service:xpath>
                        </service:ListResultField>
                        <service:ListResultField merge:matcher="skip" merge:action="insert">
                            <service:element>conditionDateLHS</service:element>
                            <service:schema>conditionchecks_lhs</service:schema>
                            <service:xpath>conditionDateLHS</service:xpath>
                        </service:ListResultField>
                    </service:ListResultsFields>
                </service:params>
            </service:DocHandlerParams>
            <service:properties xmlns:service="http://collectionspace.org/services/config/service" merge:matcher="tag" merge:action="replace">
                <types:item xmlns:types="http://collectionspace.org/services/config/types">
                    <types:key>objectNameProperty</types:key>
                    <types:value>conditionLHS</types:value>
                </types:item>
                <types:item xmlns:types="http://collectionspace.org/services/config/types">
                    <types:key>objectNumberProperty</types:key>
                    <types:value>conditionCheckRefNumber</types:value>
                </types:item>
            </service:properties>
            <service:object xmlns:service="http://collectionspace.org/services/config/service" >
                <service:part id="3" merge:matcher="skip" merge:action="insert" control_group="Managed" versionable="true" auditable="false" label="conditionchecks_lhs" updated="" order="3">
                    <service:properties>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                            <types:key>termRef</types:key>
                            <types:value>conditionLHS</types:value>
                        </types:item>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                            <types:key>termRef</types:key>
                            <types:value>estimatedCostCurrency</types:value>
                        </types:item>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                            <types:key>termRef</types:key>
                            <types:value>activityCostCurrency</types:value>
                        </types:item>
                    </service:properties>
                    <service:content contentType="application/xml">
                        <service:xmlContent namespaceURI="http://collectionspace.org/services/conditioncheck/local/lhs" schemaLocation="http://collectionspace.org/services/conditioncheck/local/lhs http://collectionspace.org/services/conditioncheck/local/conditionchecks_lhs.xsd" />
                     </service:content>
                </service:part>
            </service:object>
        </tenant:serviceBindings>

        <tenant:serviceBindings merge:matcher="id" id="Concepts">
            <service:object xmlns:service="http://collectionspace.org/services/config/service" >
                <service:part id="3" merge:matcher="skip" merge:action="insert" control_group="Managed" versionable="true" auditable="false" label="concepts_lhs" updated="" order="3">
                    <service:content contentType="application/xml">
                        <service:xmlContent namespaceURI="http://collectionspace.org/services/concept/local/lhs" schemaLocation="http://collectionspace.org/services/concept/local/lhs http://collectionspace.org/services/concept/local/concepts_lhs.xsd" />
                     </service:content>
                </service:part>
            </service:object>
        </tenant:serviceBindings>
        
        <tenant:serviceBindings merge:matcher="id" id="Acquisitions">
            <service:DocHandlerParams xmlns:service="http://collectionspace.org/services/config/service" merge:matcher="tag" merge:action="replace">
                <service:params>
                    <service:ListResultsFields>
                        <service:ListResultField>
                            <service:element>acquisitionReferenceNumber</service:element>
                            <service:xpath>acquisitionReferenceNumber</service:xpath>
                        </service:ListResultField>
                        <service:ListResultField>
                            <service:element>creditLine</service:element>
                            <service:xpath>creditLine</service:xpath>
                        </service:ListResultField>
                        <service:ListResultField>
                            <service:element>owner</service:element>
                            <service:xpath>owners/[0]</service:xpath>
                        </service:ListResultField>
                    </service:ListResultsFields>
                </service:params>
            </service:DocHandlerParams>
            <service:properties xmlns:service="http://collectionspace.org/services/config/service" merge:matcher="tag" merge:action="replace">
                <types:item xmlns:types="http://collectionspace.org/services/config/types">
                    <types:key>objectNameProperty</types:key>
                    <types:value>creditLine</types:value>
                </types:item>
                <types:item xmlns:types="http://collectionspace.org/services/config/types">
                    <types:key>objectNumberProperty</types:key>
                    <types:value>acquisitionReferenceNumber</types:value>
                </types:item>
            </service:properties>
            <service:object xmlns:service="http://collectionspace.org/services/config/service" >
                <service:part id="3" merge:matcher="skip" merge:action="insert" control_group="Managed" versionable="true" auditable="false" label="acquisitions_lhs" updated="" order="3">
                    <service:properties>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                            <types:key>authRef</types:key>
                            <types:value>acquisitionAppraiser</types:value>
                        </types:item>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                            <types:key>authRef</types:key>
                            <types:value>donor</types:value>
                        </types:item>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                            <types:key>termRef</types:key>
                            <types:value>acquisitionCostCurrency</types:value>
                        </types:item>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                            <types:key>termRef</types:key>
                            <types:value>acquisitionAppraisalCurrency</types:value>
                        </types:item>
                    </service:properties>
                    <service:content contentType="application/xml">
                        <service:xmlContent namespaceURI="http://collectionspace.org/services/acquisition/local/lhs" schemaLocation="http://collectionspace.org/services/acquisition/local/lhs http://collectionspace.org/services/acquisition/local/acquisitions_lhs.xsd" />
                     </service:content>
                </service:part>
            </service:object>
        </tenant:serviceBindings>

    </tenant:tenantBinding>
</tenant:TenantBindingConfig>
